{
  "name": "libs-bitcoin",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/libs/bitcoin/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/libs/bitcoin"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/packages/libs/bitcoin"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/libs/bitcoin"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/libs/bitcoin/**/*.ts",
          "packages/libs/bitcoin/package.json"
        ]
      }
    }
  },
  "tags": []
}
