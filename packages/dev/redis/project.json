{
  "name": "dev-redis",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/dev/redis",
  "projectType": "application",
  "implicitDependencies": ["dev-network"],
  "targets": {
    "up": {
      "command": "dev-redis up"
    },
    "sync": {
      "command": "dev-redis sync"
    },
    "logs": {
      "command": "dev-redis logs"
    },
    "recreate": {
      "command": "dev-redis reset"
    },
    "down": {
      "command": "dev-redis clean"
    }
  },
  "tags": []
}
