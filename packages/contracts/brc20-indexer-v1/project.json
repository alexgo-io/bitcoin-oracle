{
  "name": "contracts-brc20-indexer-v1",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/contracts/brc20-indexer-v1/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/contracts/brc20-indexer-v1",
        "main": "packages/contracts/brc20-indexer-v1/src/index.ts",
        "tsConfig": "packages/contracts/brc20-indexer-v1/tsconfig.lib.json",
        "assets": ["packages/contracts/brc20-indexer-v1/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/contracts/brc20-indexer-v1/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/contracts/brc20-indexer-v1/**/*.ts",
          "packages/contracts/brc20-indexer-v1/package.json"
        ]
      }
    }
  }
}
